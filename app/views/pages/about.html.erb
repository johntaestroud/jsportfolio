<div class="app-card-container">
  <div class="card">
   

  <div class="row">
    <div class="shadow">
      <%= image_tag "profile.jpg", width: "300", alt: "profile image" %>
    </div>

      <div class="col-md-8">
        <p></p>

        <p></p>

        <p>
          <button
             class="btn btn-primary"
             type="button"
             data-toggle="collapse"
             data-target="#jobHistory"
             aria-expanded="false"
             aria-controller="jobHistory"
          >
             Job History
         </button>

         <button
             class="btn btn-primary"
             type="button"
             data-toggle="collapse"
             data-target="#education"
             aria-expanded="false"
             aria-controller="education"
          >
             Education
         </button>
        </p>

        <p>
         <div class="collapse" id="jobHistory">
            <div class="card card-block">
              United States Air Force (USAFR): Feb 2012 - Current <span><br></span>
              Position: Engineering 
            </div>
            <!--<div class="card card-block">
              
            </div>-->
          </div>

          <div class="collapse" id="education">
            <div class="card card-block">
              University of Central Florida<span><br></span>
              Software Development
            </div>
            <div class="card card-block">
              Valencia College<span><br></span>
              Computer Programming & Analysis Degree<span><br></span>
              Cummulative GPA: 3.93
            </div>
          </div>
         </p>
      </div>
    </div>
  </div>
</div>




